<CompMetaData>
    <Algorithm name="WeightedAverage">
        <Comment>
            Calculate Weighted Average values (eg Water Temperature) using:
            sum(inputN*weightN) / weightTotal weightTotal is required input
            timeseries, other input timeseries obey FAIL/IGNORE specifications.
        </Comment>
        <ExecClass>decodes.tsdb.algo.WeightedAverage</ExecClass>
        <AlgoParm roleName="weightTotal">
            <ParmType>i</ParmType>
        </AlgoParm>
        <AlgoParm roleName="input1">
            <ParmType>i</ParmType>
        </AlgoParm>
        <AlgoParm roleName="input2">
            <ParmType>i</ParmType>
        </AlgoParm>
        <AlgoParm roleName="weight1">
            <ParmType>i</ParmType>
        </AlgoParm>
        <AlgoParm roleName="weight2">
            <ParmType>i</ParmType>
        </AlgoParm>
        <AlgoParm roleName="output">
            <ParmType>o</ParmType>
        </AlgoParm>
    </Algorithm>

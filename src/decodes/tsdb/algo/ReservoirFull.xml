<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<CompMetaData>
  <Algorithm name="Reservoir Full">
    <Comment>
      Given reservoir storage (output of rating computation), and a property
      &apos;capacity&apos;, output the percent full and storage remaining.
    </Comment>
    <ExecClass>decodes.tsdb.algo.ReservoirFull</ExecClass>
    <AlgoProperty name="capacity">1</AlgoProperty>
    <AlgoParm roleName="storage">
      <ParmType>i</ParmType>
    </AlgoParm>
    <AlgoParm roleName="percentFull">
      <ParmType>o</ParmType>
    </AlgoParm>
    <AlgoParm roleName="storageRemaining">
      <ParmType>o</ParmType>
    </AlgoParm>
  </Algorithm>
</CompMetaData>

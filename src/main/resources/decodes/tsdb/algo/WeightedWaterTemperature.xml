<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<CompMetaData>
  <Algorithm name="WeightedWaterTemperature">
    <Comment>
      Calculate Weighted Water Temperature using: (((input1/input2)*input3) +
      ((1 - (input1/input2))*input4))

    </Comment>
    <ExecClass>decodes.tsdb.algo.WeightedWaterTemperature</ExecClass>
    <AlgoParm roleName="input1">
      <ParmType>i</ParmType>
    </AlgoParm>
    <AlgoParm roleName="input2">
      <ParmType>i</ParmType>
    </AlgoParm>
    <AlgoParm roleName="input3">
      <ParmType>i</ParmType>
    </AlgoParm>
    <AlgoParm roleName="input4">
      <ParmType>i</ParmType>
    </AlgoParm>
    <AlgoParm roleName="output">
      <ParmType>o</ParmType>
    </AlgoParm>
  </Algorithm>
</CompMetaData>

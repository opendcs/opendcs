<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>

<!-- 
    OPENDCS 6.8 installation file.
-->

<installation version="1.0">

    <!-- 
        The info section.
    -->
    <info>
        <appname>OPENDCS</appname>
        <appversion>Open Data Collection System 6.8</appversion>
        <authors>
            <author name="Cove Software, LLC" email="info@covesw.com"/>
        </authors>
<!--
		<run-privileged condition="izpack.windowsinstall.vista|izpack.windowsinstall.7"/>
-->
        <url>http://www.covesw.com</url>
    </info>
    

    <variables>
		<variable name="DCSTOOL_HOME" value="$INSTALL_PATH"/>
		<variable name="DECODES_INSTALL_DIR" value="$INSTALL_PATH"/>
		<variable name="databasetype" value=""/>
		<variable name="databaselocation" value=""/>
		<variable name="editdatabasetype" value=""/>
		<variable name="editdatabaselocation" value=""/>
		<variable name="sitenametypepreference" value=""/>
		<variable name="editoutputformat" value=""/>
		<variable name="edittimezone" value=""/>
		<variable name="jdbcdriverclass" value=""/>
		<variable name="sqlkeygenerator" value=""/>
    </variables>

    <!-- 
        The gui preferences indication.
        Sets the installer window to 640x480. 
    -->
    <guiprefs width="720" height="480" resizable="yes"/>
    
    <!-- 
        The locale section.
        Asks here to include the English and French langpacks.
    -->
    <locale>
        <langpack iso3="eng"/>
    </locale>

    <!--
	Add native tags
    -->
    <native type="izpack" name="ShellLink_x64.dll">
	<os family="izpack.windowsinstall.7"/>
    </native>

    <native type="izpack" name="ShellLink.dll">
	<os family="windows"/>
    </native>
 
    <!-- 
        The resources section.
        The ids must be these ones if you want to use the LicencePanel 
		and/or the InfoPanel.
    -->
    <resources>
<!--
        <res id="Installer.image"  src="icons/dog.gif"/>
-->
        <res id="Installer.image"  src="icons/odcs-logo.png"/>
        <res id="shortcutSpec.xml" src="winOpenDcsShortcutSpec.xml"/>
        <res id="Unix_shortcutSpec.xml"  src="unixOpenDcsShortcutSpec.xml"/>
        <res id="Win_shortcutSpec.xml"  src="winOpenDcsShortcutSpec.xml"/>
        <res id="InfoPanel.info"  src="readme.txt"/>
        <res id="TargetPanel.dir.windows"  src="dospath.txt"/>
<!--
        <res id="LicencePanel.licence"  src="license.txt"/>
		<res id="ProcessPanel.Spec.xml" src="post-process.xml"/>
-->
    </resources>
    
    <!-- 
        The panels section.
        We indicate here which panels we want to use. 
		The order will be respected.
    -->
    <panels>
        <panel classname="HelloPanel"/>
        <panel classname="TargetPanel"/> 
        <panel classname="PacksPanel"/>
        <panel classname="InstallPanel"/>
        <panel classname="ShortcutPanel"/>
<!--
		<panel classname="ProcessPanel"/>
-->
        <panel classname="FinishPanel"/>
    </panels>
    
